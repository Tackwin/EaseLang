vect2 := struct {
	x := 0;
	y := 0;
};

rect2 := struct {
	pos  := vect2{0, 0};
	size := vect2{0, 0};
};

main := proc () {
	r     := rect2{vect2{1, 2}, vect2{3, 4}};
	r_ptr := &r;
	print(r.pos.x, r.pos.y, r.size.x, r.size.y);
	print(r_ptr.pos.x, r_ptr.pos.y, r_ptr.size.x, r_ptr.size.y);
};

main();